{
  "name": "modern_project",
  "version": "1.0.0",
  "description": "",
  "main": "dist/bundle.esm.js",
  "sideEffects": false,
  "scripts": {
    "lint": "eslint **/*.js",
    "lint:fix": "npm run lint --fix",
    "lint:watch": "esw **/*.js --watch",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coveralls": "jest --coverage && coveralls < coverage/lcov.info",
    "test:staged": "jest --findRelatedTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "npm run test:staged",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheveninKillian/Modern_project.git"
  },
  "keywords": [
    "test",
    "modern",
    "project",
    "javascript"
  ],
  "author": "Killian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheveninKillian/Modern_project/issues"
  },
  "homepage": "https://github.com/TheveninKillian/Modern_project#readme",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@rollup/plugin-babel": "^5.0.2",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "babel-jest": "^26.0.1",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-watch": "^6.0.1",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.6",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.10.9",
    "rollup-plugin-terser": "^6.1.0"
  }
}